{
  "coc.preferences.formatOnSaveFiletypes": [
    "java",
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "python",
    "html",
    "yaml",
    "markdown",
    "css"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "coc.preferences.diagnostic.virtualText": true,
  "javascript.suggestionActions.enabled": false,
  "codeLens.enable": true,
  "java.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.enable": false,
  "python.pythonPath": "python3"
}
